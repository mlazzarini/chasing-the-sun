{
  "name": "sandy-feet",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint --max-warnings 0 --report-unused-disable-directives --ignore-path .gitignore \"**/*.+(ts|js|tsx|jsx)\"",
    "type-check": "tsc",
    "type-check:watch": "tsc --watch",
    "pre-build-check": "yarn lint && yarn type-check"
  },
  "engines": {
    "node": "20.10.0"
  },
  "engineStrict": true,
  "volta": {
    "node": "20.10.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings 0 --report-unused-disable-directives --ignore-path .gitignore --fix --plugin tsc --rule 'tsc/config: [2, {configFile: \"./tsconfig.json\"}]'"
    ]
  },
  "dependencies": {
    "contentlayer": "^0.3.4",
    "date-fns": "^3.3.1",
    "next": "14.1.0",
    "next-contentlayer": "^0.3.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.17",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "packageManager": "yarn@4.0.2"
}
